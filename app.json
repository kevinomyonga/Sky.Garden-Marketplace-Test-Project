{
  "expo": {
    "name": "Sky.Garden-Marketplace-Test-Project",
    "slug": "Sky-Garden-Marketplace-Test-Project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
